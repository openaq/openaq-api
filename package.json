{
  "name": "openaq-api",
  "version": "0.1.0",
  "description": "An API for open air quality data.",
  "repository": "https://github.com/openaq/openaq-api",
  "main": "",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && mocha test/** --require babel-register",
    "local-test": "PSQL_DATABASE=openaq-test npm test",
    "start": "node index.js",
    "docs": "apidoc -i api/ -o docs/",
    "docker": "docker-compose -p openaq run --rm --service-ports api",
    "docker-test": "docker-compose -p openaq run --rm api-test npm test"
  },
  "author": "Joe Flasher",
  "license": "MIT",
  "homepage": "https://github.com/openaq/openaq-api#readme",
  "dependencies": {
    "async": "^1.5.0",
    "aws-sdk": "^2.7.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "bluebird": "^3.5.0",
    "boom": "^2.8.0",
    "csv-stringify": "0.0.8",
    "good": "^6.3.0",
    "good-winston": "^1.0.0",
    "hapi": "^13.5.3",
    "hapi-paginate": "github:developmentseed/hapi-paginate#cfa9067d634e2bcef9a631ee71646cc9dbf2f5f7",
    "hapi-rate-limiter": "^2.0.0",
    "hapi-response-meta": "^0.3.0",
    "hapi-router": "^3.0.1",
    "hiredis": "^0.4.1",
    "keen-js": "^3.3.0",
    "knex": "^0.9.0",
    "knex-postgis": "^0.1.8",
    "lodash": "^4.16.6",
    "newrelic": "^1.21.1",
    "node-redis": "^0.1.7",
    "pg": "^4.4.3",
    "redis": "^2.4.2",
    "turf-distance": "^1.1.0",
    "turf-point": "^2.0.1",
    "winston": "^2.1.1",
    "winston-papertrail": "^1.0.2"
  },
  "devDependencies": {
    "apidoc": "^0.13.1",
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "eslint": "^2.0.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    "request": "^2.67.0"
  },
  "apidoc": {
    "title": "Open AQ Platform API",
    "name": "Open AQ Platform API",
    "url": "https://api.openaq.org/v1",
    "template": {
      "withCompare": false
    }
  }
}
